<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="org.ddth.txbb.bo.Publishing" name="org.ddth.txbb.bo.Publishing"
		table="txbb_publishing">
		<cache usage="nonstrict-read-write" />
		<composite-id>
			<key-property name="boxId" type="integer" column="bid" />
			<key-property name="topicId" type="integer" column="tid" />
		</composite-id>
		<property name="timestamp" column="ttimestamp" type="integer"
			not-null="true" />
	</class>
</hibernate-mapping>
